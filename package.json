{
  "name": "basic-html-site",
  "version": "1.0.0",
  "description": "Interactive image carousel site",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist src/**/*.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "publish": "gcloud storage cp -R ./dist/* gs://basic-site-static-asset-bucket"
  },
  "keywords": [
    "typescript",
    "web-components",
    "carousel"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/dom": "^10.4.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@typescript-eslint/utils": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.28.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "happy-dom": "^17.6.3",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
